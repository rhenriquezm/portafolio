<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="modelo.Usuario" table="USUARIO">
      <id column="ID_USUARIO" name="idUsuario" type="integer" >
          <generator class="sequence">
              <param name="generator">USUARIO_SEQ</param>
          </generator>
      </id>
      <property column="USER_USUARIO" name="userUsuario"/>
      <property column="PASS_USUARIO" name="passUsuario"/>
      <property column="RUT_USUARIO" name="rutUsuario"/>
      <property column="DV_USUARIO" name="dvUsuario"/>
      <property column="NOM_USUARIO" name="nomUsuario"/>
      <property column="PAT_USUARIO" name="apelPUsuario"/>
      <property column="MAT_USUARIO" name="apelMUsuario"/>
      <property column="SEXO_USUARIO" name="sexoUsuario" />
      <property column="CORREO_USUARIO" name="correoUsuario"/>
      <property column="FONO_USUARIO" name="fonoUsuario"/>
      <property column="ID_PERFIL" name="idPerfil"/>
      <property column="ID_UNI_TRAB" name="idUniTrab"/>
  </class>
</hibernate-mapping>
